    "Table": {
        "prefix": "table",
        "body": [
            "\\begin{table}",
            "\t\\caption{$1}\\label{tab:$2}",
            "\t\\begin{center}",
            "\t\t\\begin{tabular}[c]{l|l}",
            "\t\t\t\\hline",
            "\t\t\t\\multicolumn{1}{c|}{\\textbf{$3}} & ",
            "\t\t\t\\multicolumn{1}{c}{\\textbf{$4}} \\\\\\\\",
            "\t\t\t\\hline",
            "\t\t\ta & b \\\\\\\\",
            "\t\t\tc & d \\\\\\\\",
            "\t\t\t$5",
            "\t\t\t\\hline",
            "\t\t\\end{tabular}",
            "\t\\end{center}",
            "\\end{table}",
            "$0"
        ],
        "description": "Add a table"
    },
