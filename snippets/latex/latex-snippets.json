{
    "Display Math — \\[ … \\]": {
        "prefix": "$$",
        "body": ["\\[", "\t$1", "\\]"],
        "description": "Display Math"
    },
    "Inline Math — \\$ … \\$": {
        "prefix": "mk",
        "body": ["\\$ $1 \\$"],
        "description": "Inline Math"
    },
    "Theorem": {
        "prefix": "theorem",
        "body": [
            "\\begin{theorem}[$1]",
            "\t$2",
            "\t\\[",
            "\t\t$3",
            "\t\\]",
            "\t$4",
            "\\end{theorem}",
            "$0"
        ],
        "description": "Add a theorem"
    },
    "Definition": {
        "prefix": "definition",
        "body": [
            "\\begin{definition}[$1]",
            "\t$2",
            "\t\\[",
            "\t\t$3",
            "\t\\]",
            "\t$4",
            "\\end{definition}",
            "$0"
        ],
        "description": "Add a definition"
    },
    "Proof": {
        "prefix": "proof",
        "body": [
            "\\begin{proof}",
            "\t$1",
            "\t\\[",
            "\t\t$2",
            "\t\\]",
            "\t$3",
            "\\end{proof}",
            "$0"
        ],
        "description": "Add a proof"
    },
    "Figure:Ref": {
        "prefix": "figure:ref",
        "body": ["${1:Figure}~\\ref{fig:$2}$0"],
        "description": "Ref for Figure"
    },
    "Matrix": {
        "prefix": "mat",
        "body": [
            "\\begin{${1:p/b/v/V/B/small}matrix}",
            "\t$0",
            "\\end{${1:p/b/v/V/B/small}matrix}"
        ],
        "description": "Matrix"
    },
    "Table": {
        "prefix": "table",
        "body": [
            "\\begin{table}",
            "\t\\caption{$1}\\label{tab:$2}",
            "\t\\begin{center}",
            "\t\t\\begin{tabular}[c]{l|l}",
            "\t\t\t\\hline",
            "\t\t\t\\multicolumn{1}{c|}{\\textbf{$3}} & ",
            "\t\t\t\\multicolumn{1}{c}{\\textbf{$4}} \\\\\\\\",
            "\t\t\t\\hline",
            "\t\t\ta & b \\\\\\\\",
            "\t\t\tc & d \\\\\\\\",
            "\t\t\t$5",
            "\t\t\t\\hline",
            "\t\t\\end{tabular}",
            "\t\\end{center}",
            "\\end{table}",
            "$0"
        ],
        "description": "Add a table"
    },
    "Table*": {
        "prefix": "table*",
        "body": [
            "\\begin{table*}",
            "\t\\caption{$1}\\label{tab:$2}",
            "\t\\begin{center}",
            "\t\t\\begin{tabular}[c]{l|l}",
            "\t\t\t\\hline",
            "\t\t\t\\multicolumn{1}{c|}{\\textbf{$3}} & ",
            "\t\t\t\\multicolumn{1}{c}{\\textbf{$4}} \\\\\\\\",
            "\t\t\t\\hline",
            "\t\t\ta & b \\\\\\\\",
            "\t\t\tc & d \\\\\\\\",
            "\t\t\t$5",
            "\t\t\t\\hline",
            "\t\t\\end{tabular}",
            "\t\\end{center}",
            "\\end{table*}",
            "$0"
        ],
        "description": "Add a table"
    },
    "Table:ACM": {
        "prefix": "table:acm",
        "body": [
            "\\begin{table}",
            "\t\\caption{$1}\\label{tab:$2}",
            "\t\\begin{tabular}{${3:ccl}}",
            "\t\t\\toprule",
            "\t\t$4",
            "\t\ta & b & c \\\\\\\\",
            "\t\t\\midrule",
            "\t\td & e & f \\\\\\\\",
            "\t\t\\bottomrule",
            "\t\\end{tabular}",
            "\\end{table}",
            "$0"
        ],
        "description": "Add a table (ACM)"
    },
    "Table:ACM:*": {
        "prefix": "table:acm:*",
        "body": [
            "\\begin{table*}",
            "\t\\caption{$1}\\label{tab:$2}",
            "\t\\begin{tabular}{${3:ccl}}",
            "\t\t\\toprule",
            "\t\t$4",
            "\t\ta & b & c \\\\\\\\",
            "\t\t\\midrule",
            "\t\td & e & f \\\\\\\\",
            "\t\t\\bottomrule",
            "\t\\end{tabular}",
            "\\end{table*}",
            "$0"
        ],
        "description": "Add a table (ACM)"
    },
    "Cite": {
        "prefix": "cite",
        "body": ["\\cite{$1}$0"],
        "description": "Add a cite"
    },
}
